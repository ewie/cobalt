prefix :     <https://vsr.informatik.tu-chemnitz.de/projects/2014/cobalt/mashup#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>

select ?property ?type
where {
  ?property :hasType ?type .
  filter not exists { ?property :hasName [] }
}

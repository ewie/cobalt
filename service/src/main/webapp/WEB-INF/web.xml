<?xml version="1.0" encoding="UTF-8"?>
<web-app
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    metadata-complete="false"
    version="3.0">

  <display-name>Cobalt Planner</display-name>

  <listener>
    <listener-class>vsr.cobalt.service.app.Listener</listener-class>
  </listener>

  <servlet>
    <servlet-name>PlannerServlet</servlet-name>
    <servlet-class>vsr.cobalt.service.app.PlannerServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>PlannerServlet</servlet-name>
    <url-pattern>/planner</url-pattern>
  </servlet-mapping>

</web-app>
